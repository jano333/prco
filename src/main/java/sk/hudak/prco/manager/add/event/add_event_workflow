*
*db keyword id convert to keyword it self which will be use to search product on eshop
*
NewKeywordIdEvent => NewKeyWordEvent
                  =# RetrieveKeywordBaseOnKeywordIdErrorEvent
*
* convert keyword to search keyword URL
*
NewKeywordEvent => SearchKeywordUrlEvent
                =# BuildSearchUrlForKeywordErrorEvent

SearchKeywordUrlEvent => SearchPageDocumentEvent
                      =# RetrieveDocumentForSearchUrlErrorEvent

SearchPageDocumentEvent =?> CountOfPagesEvent           if( pageNumber is 1)
                        => NewProductEshopUrlsEvent
                        =# ParseCountOfPagesErrorEvent
                        =# ParseProductListURLsErrorEvent

NewProductEshopUrlsEvent =?> [*]NewProductUrlWithEshopEvent if (none duplicity, none existance)
                         =# FilterDuplicityErrorEvent
                         =# FilterNotExistingErrorEvent

CountOfPagesEvent => [*]BuildNextSearchPageUrlEvent
                  =# nothing

NewProductUrlWithEshopEvent => NewProductDocumentEvent
                            =# RetrieveDocumentForUrlErrorEvent

BuildNextSearchPageUrlEvent => SearchKeywordUrlEvent
                            =# BuildNextPageSearchUrlErrorEvent

NewProductDocumentEvent => ProductNewDataEvent
                        =# ParseProductNewDataErrorEvent

ProductNewDataEvent => SaveProductNewDataEvent
                    =# SaveProductNewDataErrorEvent

SaveProductNewDataEvent => END

**************************************************************************************
NewProductUrlsEvent => NewProductUrlEvent
                    =# FilterNotExistingProductErrorEvent

NewProductUrlEvent => NewProductUrlWithEshopEvent
                   =# ParseEshopUuidErrorEvent
**************************************************************************************
